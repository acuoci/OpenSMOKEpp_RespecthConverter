Dictionary PlugFlowReactor
{
        @KineticsFolder          $PATHKINETICFOLDER$;
        @Type                    NonIsothermal;
        @InletStatus             inlet-status;
        @ResidenceTime           $MISSING$;
        @ConstantPressure        true;
        @Velocity                10 cm/s;
        @Options                 output-options;
        @ParametricAnalysis      parametric-analysis;
}

Dictionary inlet-status
{
        @Temperature      9.631500e+02 K ;
        @Pressure         9.700000e-01 bar ;
        @MoleFractions    AR 9.300000e-01 O2 5.800000e-02 DMM 1.200000e-02 ;
}

